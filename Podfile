#use_frameworks!
platform :ios, ‘8.0’
source 'https://github.com/CocoaPods/Specs.git'

inhibit_all_warnings!
def dev_pods
	# Data
	pod 'AFNetworking'
	pod 'CocoaLumberjack'
	pod 'CocoaSecurity', '~> 1.2.3'
    pod 'iAppInfos', '~> 0.0.6'
	pod 'SDWebImage'
	pod 'RongCloudIMKit’
	pod 'YYCache'
	pod 'JSONModel'

	# UI
	pod 'M13BadgeView'
	pod 'MBProgressHUD'
	pod 'MJRefresh'
	pod 'TZImagePickerController'
	pod 'Masonry'
	pod 'MGSwipeTableCell'
end


target 'FakeWechat' do
    dev_pods
end
